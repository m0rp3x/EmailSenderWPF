<Window x:Class="EmailSenderWPF.Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EmailSenderWPF"
        mc:Ignorable="d"
        Title="Window1" Height="232" Width="800">
    <Grid>
        <TextBox x:Name="Email" HorizontalAlignment="Left" Margin="60,26,0,0" TextWrapping="Wrap" Text="Адресс Почты" VerticalAlignment="Top" Width="178" TextChanged="TextBox_TextChanged"/>
        <TextBox x:Name="Password" TextWrapping="Wrap"  Margin="60,56,562,136"/>
        <Button x:Name="Login" Content="Логин" HorizontalAlignment="Left" Margin="60,93,0,0" VerticalAlignment="Top" Width="178" Click="Login_Click"/>

    </Grid>
</Window>
